function _getRequireWildcardCache(nodeInterop) {
  /* istanbul ignore next */
  cov_oxjagn8zk().f[3]++;
  cov_oxjagn8zk().s[22]++;
  if (typeof WeakMap !== "function") {
    /* istanbul ignore next */
    cov_oxjagn8zk().b[0][0]++;
    cov_oxjagn8zk().s[23]++;
    return null;
  } else
  /* istanbul ignore next */
  {
    cov_oxjagn8zk().b[0][1]++;
  }
  var cacheBabelInterop =
  /* istanbul ignore next */
  (cov_oxjagn8zk().s[24]++, new WeakMap());
  var cacheNodeInterop =
  /* istanbul ignore next */
  (cov_oxjagn8zk().s[25]++, new WeakMap());
  /* istanbul ignore next */
  cov_oxjagn8zk().s[26]++;
  return (_getRequireWildcardCache = function (nodeInterop) {
    /* istanbul ignore next */
    cov_oxjagn8zk().f[4]++;
    cov_oxjagn8zk().s[27]++;
    return nodeInterop ?
    /* istanbul ignore next */
    (cov_oxjagn8zk().b[1][0]++, cacheNodeInterop) :
    /* istanbul ignore next */
    (cov_oxjagn8zk().b[1][1]++, cacheBabelInterop);
  })(nodeInterop);
}
function _interop_require_wildcard(obj, nodeInterop) {
  /* istanbul ignore next */
  cov_oxjagn8zk().f[5]++;
  cov_oxjagn8zk().s[28]++;
  if (
  /* istanbul ignore next */
  (cov_oxjagn8zk().b[3][0]++, !nodeInterop) &&
  /* istanbul ignore next */
  (cov_oxjagn8zk().b[3][1]++, obj) &&
  /* istanbul ignore next */
  (cov_oxjagn8zk().b[3][2]++, obj.__esModule)) {
    /* istanbul ignore next */
    cov_oxjagn8zk().b[2][0]++;
    cov_oxjagn8zk().s[29]++;
    return obj;
  } else
  /* istanbul ignore next */
  {
    cov_oxjagn8zk().b[2][1]++;
  }
  cov_oxjagn8zk().s[30]++;
  if (
  /* istanbul ignore next */
  (cov_oxjagn8zk().b[5][0]++, obj === null) ||
  /* istanbul ignore next */
  (cov_oxjagn8zk().b[5][1]++, typeof obj !== "object") &&
  /* istanbul ignore next */
  (cov_oxjagn8zk().b[5][2]++, typeof obj !== "function")) {
    /* istanbul ignore next */
    cov_oxjagn8zk().b[4][0]++;
    cov_oxjagn8zk().s[31]++;
    return {
      default: obj
    };
  } else
  /* istanbul ignore next */
  {
    cov_oxjagn8zk().b[4][1]++;
  }
  var cache =
  /* istanbul ignore next */
  (cov_oxjagn8zk().s[32]++, _getRequireWildcardCache(nodeInterop));
  /* istanbul ignore next */
  cov_oxjagn8zk().s[33]++;
  if (
  /* istanbul ignore next */
  (cov_oxjagn8zk().b[7][0]++, cache) &&
  /* istanbul ignore next */
  (cov_oxjagn8zk().b[7][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_oxjagn8zk().b[6][0]++;
    cov_oxjagn8zk().s[34]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_oxjagn8zk().b[6][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_oxjagn8zk().s[35]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_oxjagn8zk().s[36]++,
  /* istanbul ignore next */
  (cov_oxjagn8zk().b[8][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_oxjagn8zk().b[8][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_oxjagn8zk().s[37]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_oxjagn8zk().s[38]++;
    if (
    /* istanbul ignore next */
    (cov_oxjagn8zk().b[10][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_oxjagn8zk().b[10][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
      /* istanbul ignore next */
      cov_oxjagn8zk().b[9][0]++;
      var desc =
      /* istanbul ignore next */
      (cov_oxjagn8zk().s[39]++, hasPropertyDescriptor ?
      /* istanbul ignore next */
      (cov_oxjagn8zk().b[11][0]++, Object.getOwnPropertyDescriptor(obj, key)) :
      /* istanbul ignore next */
      (cov_oxjagn8zk().b[11][1]++, null));
      /* istanbul ignore next */
      cov_oxjagn8zk().s[40]++;
      if (
      /* istanbul ignore next */
      (cov_oxjagn8zk().b[13][0]++, desc) && (
      /* istanbul ignore next */
      (cov_oxjagn8zk().b[13][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_oxjagn8zk().b[13][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_oxjagn8zk().b[12][0]++;
        cov_oxjagn8zk().s[41]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_oxjagn8zk().b[12][1]++;
        cov_oxjagn8zk().s[42]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_oxjagn8zk().b[9][1]++;
    }
  }
  /* istanbul ignore next */
  cov_oxjagn8zk().s[43]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_oxjagn8zk().s[44]++;
  if (cache) {
    /* istanbul ignore next */
    cov_oxjagn8zk().b[14][0]++;
    cov_oxjagn8zk().s[45]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_oxjagn8zk().b[14][1]++;
  }
  cov_oxjagn8zk().s[46]++;
  return newObj;
}
