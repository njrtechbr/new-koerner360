oid;\n  readonly?: boolean;\n}\n\n/**\n * Componente para configurar prefer\xEAncias de e-mail de notifica\xE7\xF5es\n */\nexport function ConfiguracaoEmail({\n  usuarioId,\n  configuracaoInicial,\n  onSalvar,\n  onTestar,\n  readonly = false\n}: ConfiguracaoEmailProps) {\n  const [configuracao, setConfiguracao] = useState<ConfiguracaoEmailUsuario>({\n    usuarioId,\n    emailAtivo: true,\n    emailPrincipal: '',\n    emailsAdicionais: [],\n    frequenciaNotificacoes: 'imediata',\n    tiposHabilitados: ['avaliacao_pendente', 'lembrete_prazo'],\n    horarioPreferido: '09:00',\n    diasAntecedencia: 3,\n    incluirResumo: true,\n    formatoHtml: true,\n    ...configuracaoInicial\n  });\n\n  const [novoEmail, setNovoEmail] = useState('');\n  const [salvando, setSalvando] = useState(false);\n  const [testando, setTestando] = useState(false);\n  const [alteracoesPendentes, setAlteracoesPendentes] = useState(false);\n\n  const { enviarNotificacaoAvaliacaoPendente } = useEmailNotificacoes();\n  const { estatisticas, carregarEstatisticas, obterEstatisticasGerais } = useEstatisticasEmail();\n\n  // Carrega estat\xEDsticas ao montar o componente\n  useEffect(() => {\n    carregarEstatisticas();\n  }, [carregarEstatisticas]);\n\n  // Monitora altera\xE7\xF5es na configura\xE7\xE3o\n  useEffect(() => {\n    setAlteracoesPendentes(true);\n  }, [configuracao]);\n\n  /**\n   * Atualiza campo da configura\xE7\xE3o\n   */\n  const atualizarConfiguracao = <K extends keyof ConfiguracaoEmailUsuario>(\n    campo: K,\n    valor: ConfiguracaoEmailUsuario[K]\n  ) => {\n    if (readonly) return;\n    \n    setConfiguracao(prev => ({\n      ...prev,\n      [campo]: valor\n    }));\n  };\n\n  /**\n   * Adiciona e-mail adicional\n   */\n  const adicionarEmail = () => {\n    if (!novoEmail || !novoEmail.includes('@')) {\n      toast.error('Digite um e-mail v\xE1lido');\n      return;\n    }\n\n    if (configuracao.emailsAdicionais.includes(novoEmail)) {\n      toast.error('Este e-mail j\xE1 foi adicionado');\n      return;\n    }\n\n    atualizarConfiguracao('emailsAdicionais', [...configuracao.emailsAdicionais, novoEmail]);\n    setNovoEmail('');\n    toast.success('E-mail adicionado');\n  };\n\n  /**\n   * Remove e-mail adicional\n   */\n  const removerEmail = (email: string) => {\n    atualizarConfiguracao(\n      'emailsAdicionais',\n      configuracao.emailsAdicionais.filter(e => e !== email)\n    );\n    toast.success('E-mail removido');\n  };\n\n  /**\n   * Alterna tipo de notifica\xE7\xE3o\n   */\n  const alternarTipoNotificacao = (tipo: TipoNotificacaoEmail) => {\n    const tiposAtuais = configuracao.tiposHabilitados;\n    const novosTipos = tiposAtuais.includes(tipo)\n      ? tiposAtuais.filter(t => t !== tipo)\n      : [...tiposAtuais, tipo];\n    \n    atualizarConfiguracao('tiposHabilitados', novosTipos);\n  };\n\n  /**\n   * Salva configura\xE7\xE3o\n   */\n  const salvarConfiguracao = async () => {\n    if (!configuracao.emailPrincipal) {\n      toast.error('E-mail principal \xE9 obrigat\xF3rio');\n      return;\n    }\n\n    setSalvando(true);\n    try {\n      await onSalvar?.(configuracao);\n      setAlteracoesPendentes(false);\n      toast.success('Configura\xE7\xE3o salva com sucesso');\n    } catch (error) {\n      toast.error('Erro ao salvar configura\xE7\xE3o');\n      console.error('Erro ao salvar:', error);\n    } finally {\n      setSalvando(false);\n    }\n  };\n\n  /**\n   * Testa envio de e-mail\n   */\n  const testarEnvio = async () => {\n    if (!configuracao.emailPrincipal) {\n      toast.error('Configure um e-mail principal primeiro');\n      return;\n    }\n\n    setTestando(true);\n    try {\n      await onTestar?.(configuracao.emailPrincipal);\n      toast.success('E-mail de teste enviado');\n    } catch (error) {\n      toast.error('Erro ao enviar e-mail de teste');\n      console.error('Erro no teste:', error);\n    } finally {\n      setTestando(false);\n    }\n  };\n\n  /**\n   * Reseta configura\xE7\xE3o\n   */\n  const resetarConfiguracao = () => {\n    setConfiguracao({\n      usuarioId,\n      emailAtivo: true,\n      emailPrincipal: '',\n      emailsAdicionais: [],\n      frequenciaNotificacoes: 'imediata',\n      tiposHabilitados: ['avaliacao_pendente', 'lembrete_prazo'],\n      horarioPreferido: '09:00',\n      diasAntecedencia: 3,\n      incluirResumo: true,\n      formatoHtml: true,\n      ...configuracaoInicial\n    });\n    setAlteracoesPendentes(false);\n    toast.success('Configura\xE7\xE3o resetada');\n  };\n\n  const estatisticasGerais = obterEstatisticasGerais();\n\n  const tiposNotificacao = [\n    {\n      tipo: 'avaliacao_pendente' as TipoNotificacaoEmail,\n      label: 'Avalia\xE7\xE3o Pendente',\n      descricao: 'Notifica quando uma nova avalia\xE7\xE3o \xE9 atribu\xEDda',\n      icone: Bell\n    },\n    {\n      tipo: 'lembrete_prazo' as TipoNotificacaoEmail,\n      label: 'Lembrete de Prazo',\n      descricao: 'Lembra sobre prazos pr\xF3ximos de avalia\xE7\xF5es',\n      icone: Clock\n    },\n    {\n      tipo: 'avaliacao_vencida' as TipoNotificacaoEmail,\n      label: 'Avalia\xE7\xE3o Vencida',\n      descricao: 'Notifica sobre avalia\xE7\xF5es que venceram',\n      icone: AlertTriangle\n    },\n    {\n      tipo: 'resumo_semanal' as TipoNotificacaoEmail,\n      label: 'Resumo Semanal',\n      descricao: 'Resumo semanal de todas as avalia\xE7\xF5es',\n      icone: Calendar\n    }\n  ];\n\n  return (\n    <div className=\"space-y-6\">\n      {/* Header */}\n      <div className=\"flex items-center justify-between\">\n        <div className=\"flex items-center gap-2\">\n          <Mail className=\"h-5 w-5\" />\n          <h2 className=\"text-lg font-semibold\">Configura\xE7\xF5es de E-mail</h2>\n        </div>\n        \n        {!readonly && (\n          <div className=\"flex items-center gap-2\">\n            {alteracoesPendentes && (\n              <Badge variant=\"outline\" className=\"text-orange-600\">\n                <Info className=\"h-3 w-3 mr-1\" />\n                Altera\xE7\xF5es pendentes\n              </Badge>\n            )}\n            \n            <Button\n              variant=\"outline\"\n              size=\"sm\"\n              onClick={resetarConfiguracao}\n              disabled={salvando}\n            >\n              <RotateCcw className=\"h-4 w-4 mr-1\" />\n              Resetar\n            </Button>\n            \n            <Button\n              variant=\"outline\"\n              size=\"sm\"\n              onClick={testarEnvio}\n              disabled={testando || !configuracao.emailPrincipal}\n            >\n              <TestTube className=\"h-4 w-4 mr-1\" />\n              {testando ? 'Testando...' : 'Testar'}\n            </Button>\n            \n            <Button\n              onClick={salvarConfiguracao}\n              disabled={salvando || !alteracoesPendentes}\n            >\n              <Save className=\"h-4 w-4 mr-1\" />\n              {salvando ? 'Salvando...' : 'Salvar'}\n            </Button>\n          </div>\n        )}\n      </div>\n\n      <Tabs defaultValue=\"geral\" className=\"space-y-4\">\n        <TabsList>\n          <TabsTrigger value=\"geral\">Geral</TabsTrigger>\n          <TabsTrigger value=\"tipos\">Tipos de Notifica\xE7\xE3o</TabsTrigger>\n          <TabsTrigger value=\"horarios\">Hor\xE1rios</TabsTrigger>\n          <TabsTrigger value=\"estatisticas\">Estat\xEDsticas</TabsTrigger>\n        </TabsList>\n\n        {/* Aba Geral */}\n        <TabsContent value=\"geral\" className=\"space-y-4\">\n          <Card>\n            <CardHeader>\n              <CardTitle className=\"flex items-center gap-2\">\n                <Settings className=\"h-4 w-4\" />\n                Configura\xE7\xF5es Gerais\n              </CardTitle>\n              <CardDescription>\n                Configure suas prefer\xEAncias b\xE1sicas de e-mail\n              </CardDescription>\n            </CardHeader>\n            <CardContent className=\"space-y-4\">\n              {/* Status Ativo */}\n              <div className=\"flex items-center justify-between\">\n                <div className=\"space-y-0.5\">\n                  <Label>Notifica\xE7\xF5es por E-mail</Label>\n                  <p className=\"text-sm text-muted-foreground\">\n                    Ativar ou desativar todas as notifica\xE7\xF5es por e-mail\n                  </p>\n                </div>\n                <Switch\n                  checked={configuracao.emailAtivo}\n                  onCheckedChange={(checked) => atualizarConfiguracao('emailAtivo', checked)}\n                  disabled={readonly}\n                />\n              </div>\n\n              <Separator />\n\n              {/* E-mail Principal */}\n              <div className=\"space-y-2\">\n                <Label htmlFor=\"email-principal\">E-mail Principal *</Label>\n                <Input\n                  id=\"email-principal\"\n                  type=\"email\"\n                  value={configuracao.emailPrincipal}\n                  onChange={(e) => atualizarConfiguracao('emailPrincipal', e.target.value)}\n                  placeholder=\"seu.email@empresa.com\"\n                  disabled={readonly}\n                />\n                <p className=\"text-xs text-muted-foreground\">\n                  E-mail principal para receber notifica\xE7\xF5es\n                </p>\n              </div>\n\n              {/* E-mails Adicionais */}\n              <div className=\"space-y-2\">\n                <Label>E-mails Adicionais</Label>\n                \n                {!readonly && (\n                  <div className=\"flex gap-2\">\n                    <Input\n                      type=\"email\"\n                      value={novoEmail}\n                      onChange={(e) => setNovoEmail(e.target.value)}\n                      placeholder=\"email.adicional@empresa.com\"\n                      onKeyPress={(e) => e.key === 'Enter' && adicionarEmail()}\n                    />\n                    <Button\n                      type=\"button\"\n                      variant=\"outline\"\n                      onClick={adicionarEmail}\n                      disabled={!novoEmail}\n                    >\n                      Adicionar\n                    </Button>\n                  </div>\n                )}\n                \n                {configuracao.emailsAdicionais.length > 0 && (\n                  <div className=\"flex flex-wrap gap-2\">\n                    {configuracao.emailsAdicionais.map((email, index) => (\n                      <Badge key={index} variant=\"secondary\" className=\"flex items-center gap-1\">\n                        {email}\n                        {!readonly && (\n                          <XCircle\n                            className=\"h-3 w-3 cursor-pointer hover:text-destructive\"\n                            onClick={() => removerEmail(email)}\n                          />\n                        )}\n                      </Badge>\n                    ))}\n                  </div>\n                )}\n                \n                <p className=\"text-xs text-muted-foreground\">\n                  E-mails adicionais que tamb\xE9m receber\xE3o c\xF3pias das notifica\xE7\xF5es\n                </p>\n              </div>\n\n              {/* Frequ\xEAncia */}\n              <div className=\"space-y-2\">\n                <Label>Frequ\xEAncia de Notifica\xE7\xF5es</Label>\n                <Select\n                  value={configuracao.frequenciaNotificacoes}\n                  onValueChange={(value: 'imediata' | 'diaria' | 'semanal') => \n                    atualizarConfiguracao('frequenciaNotificacoes', value)\n                  }\n                  disabled={readonly}\n                >\n                  <SelectTrigger>\n                    <SelectValue />\n                  </SelectTrigger>\n                  <SelectContent>\n                    <SelectItem value=\"imediata\">Imediata</SelectItem>\n                    <SelectItem value=\"diaria\">Di\xE1ria</SelectItem>\n                    <SelectItem value=\"semanal\">Semanal</SelectItem>\n                  </SelectContent>\n                </Select>\n                <p className=\"text-xs text-muted-foreground\">\n                  Com que frequ\xEAncia voc\xEA deseja receber as notifica\xE7\xF5es\n                </p>\n              </div>\n\n              {/* Formato */}\n              <div className=\"flex items-center justify-between\">\n                <div className=\"space-y-0.5\">\n                  <Label>Formato HTML</Label>\n                  <p className=\"text-sm text-muted-foreground\">\n                    Receber e-mails em formato HTML (mais visual)\n                  </p>\n                </div>\n                <Switch\n                  checked={configuracao.formatoHtml}\n                  onCheckedChange={(checked) => atualizarConfiguracao('formatoHtml', checked)}\n                  disabled={readonly}\n                />\n              </div>\n\n              {/* Incluir Resumo */}\n              <div className=\"flex items-center justify-between\">\n                <div className=\"space-y-0.5\">\n                  <Label>Incluir Resumo</Label>\n                  <p className=\"text-sm text-muted-foreground\">\n                    Incluir resumo de atividades nos e-mails\n                  </p>\n                </div>\n                <Switch\n                  checked={configuracao.incluirResumo}\n                  onCheckedChange={(checked) => atualizarConfiguracao('incluirResumo', checked)}\n                  disabled={readonly}\n                />\n              </div>\n            </CardContent>\n          </Card>\n        </TabsContent>\n\n        {/* Aba Tipos de Notifica\xE7\xE3o */}\n        <TabsContent value=\"tipos\" className=\"space-y-4\">\n          <Card>\n            <CardHeader>\n              <CardTitle className=\"flex items-center gap-2\">\n                <Bell className=\"h-4 w-4\" />\n                Tipos de Notifica\xE7\xE3o\n              </CardTitle>\n              <CardDescription>\n                Escolha quais tipos de notifica\xE7\xE3o voc\xEA deseja receber\n              </CardDescription>\n            </CardHeader>\n            <CardContent className=\"space-y-4\">\n              {tiposNotificacao.map(({ tipo, label, descricao, icone: Icone }) => {\n                const ativo = configuracao.tiposHabilitados.includes(tipo);\n                \n                return (\n                  <div key={tipo} className=\"flex items-center justify-between p-3 border rounded-lg\">\n                    <div className=\"flex items-center gap-3\">\n                      <Icone className={`h-5 w-5 ${ativo ? 'text-primary' : 'text-muted-foreground'}`} />\n                      <div>\n                        <p className=\"font-medium\">{label}</p>\n                        <p className=\"text-sm text-muted-foreground\">{descricao}</p>\n                      </div>\n                    </div>\n                    <Switch\n                      checked={ativo}\n                      onCheckedChange={() => alternarTipoNotificacao(tipo)}\n                      disabled={readonly}\n                    />\n                  </div>\n                );\n              })}\n            </CardContent>\n          </Card>\n        </TabsContent>\n\n        {/* Aba Hor\xE1rios */}\n        <TabsContent value=\"horarios\" className=\"space-y-4\">\n          <Card>\n            <CardHeader>\n              <CardTitle className=\"flex items-center gap-2\">\n                <Clock className=\"h-4 w-4\" />\n                Configura\xE7\xF5es de Hor\xE1rio\n              </CardTitle>\n              <CardDescription>\n                Configure quando e com que anteced\xEAncia receber notifica\xE7\xF5es\n              </CardDescription>\n            </CardHeader>\n            <CardContent className=\"space-y-4\">\n              {/* Hor\xE1rio Preferido */}\n              <div className=\"space-y-2\">\n                <Label htmlFor=\"horario-preferido\">Hor\xE1rio Preferido</Label>\n                <Input\n                  id=\"horario-preferido\"\n                  type=\"time\"\n                  value={configuracao.horarioPreferido}\n                  onChange={(e) => atualizarConfiguracao('horarioPreferido', e.target.value)}\n                  disabled={readonly}\n                />\n                <p className=\"text-xs text-muted-foreground\">\n                  Hor\xE1rio preferido para receber notifica\xE7\xF5es di\xE1rias\n                </p>\n              </div>\n\n              {/* Dias de Anteced\xEAncia */}\n              <div className=\"space-y-2\">\n                <Label htmlFor=\"dias-antecedencia\">Dias de Anteced\xEAncia</Label>\n                <Input\n                  id=\"dias-antecedencia\"\n                  type=\"number\"\n                  min=\"1\"\n                  max=\"30\"\n                  value={configuracao.diasAntecedencia}\n                  onChange={(e) => atualizarConfiguracao('diasAntecedencia', parseInt(e.target.value) || 3)}\n                  disabled={readonly}\n                />\n                <p className=\"text-xs text-muted-foreground\">\n                  Quantos dias antes do prazo voc\xEA deseja ser notificado\n                </p>\n              </div>\n\n              {/* Assinatura */}\n              <div className=\"space-y-2\">\n                <Label htmlFor=\"assinatura\">Assinatura Personalizada</Label>\n                <Textarea\n                  id=\"assinatura\"\n                  value={configuracao.assinatura || ''}\n                  onChange={(e) => atualizarConfiguracao('assinatura', e.target.value)}\n                  placeholder=\"Sua assinatura personalizada...\"\n                  rows={3}\n                  disabled={readonly}\n                />\n                <p className=\"text-xs text-muted-foreground\">\n                  Assinatura que ser\xE1 inclu\xEDda no final dos e-mails (opcional)\n                </p>\n              </div>\n            </CardContent>\n          </Card>\n        </TabsContent>\n\n        {/* Aba Estat\xEDsticas */}\n        <TabsContent value=\"estatisticas\" className=\"space-y-4\">\n          <Card>\n            <CardHeader>\n              <CardTitle className=\"flex items-center gap-2\">\n                <Users className=\"h-4 w-4\" />\n                Estat\xEDsticas de E-mail\n              </CardTitle>\n              <CardDescription>\n                Acompanhe o hist\xF3rico de envios de e-mail\n              </CardDescription>\n            </CardHeader>\n            <CardContent>\n              <div className=\"grid grid-cols-1 md:grid-cols-3 gap-4\">\n                <div className=\"text-center p-4 border rounded-lg\">\n                  <div className=\"flex items-center justify-center mb-2\">\n                    <Mail className=\"h-5 w-5 text-blue-500\" />\n                  </div>\n                  <p className=\"text-2xl font-bold\">{estatisticasGerais.totalEnviados}</p>\n                  <p className=\"text-sm text-muted-foreground\">Total Enviados</p>\n                </div>\n                \n                <div className=\"text-center p-4 border rounded-lg\">\n                  <div className=\"flex items-center justify-center mb-2\">\n                    <CheckCircle className=\"h-5 w-5 text-green-500\" />\n                  </div>\n                  <p className=\"text-2xl font-bold\">{estatisticasGerais.sucessos}</p>\n                  <p className=\"text-sm text-muted-foreground\">Sucessos</p>\n                </div>\n                \n                <div className=\"text-center p-4 border rounded-lg\">\n                  <div className=\"flex items-center justify-center mb-2\">\n                    <XCircle className=\"h-5 w-5 text-red-500\" />\n                  </div>\n                  <p className=\"text-2xl font-bold\">{estatisticasGerais.falhas}</p>\n                  <p className=\"text-sm text-muted-foreground\">Falhas</p>\n                </div>\n              </div>\n              \n              {estatisticasGerais.totalEnviados > 0 && (\n                <div className=\"mt-4 p-4 bg-muted rounded-lg\">\n                  <div className=\"flex items-center justify-between mb-2\">\n                    <span className=\"text-sm font-medium\">Taxa de Sucesso</span>\n                    <span className=\"text-sm font-bold\">\n                      {estatisticasGerais.taxaSucesso.toFixed(1)}%\n                    </span>\n                  </div>\n                  <div className=\"w-full bg-background rounded-full h-2\">\n                    <div\n                      className=\"bg-green-500 h-2 rounded-full transition-all\"\n                      style={{ width: `${estatisticasGerais.taxaSucesso}%` }}\n                    />\n                  </div>\n                </div>\n              )}\n              \n              {estatisticasGerais.ultimoEnvio && (\n                <div className=\"mt-4 text-center text-sm text-muted-foreground\">\n                  \xDAltimo envio: {estatisticasGerais.ultimoEnvio.toLocaleString('pt-BR')}\n                </div>\n              )}\n            </CardContent>\n          </Card>\n        </TabsContent>\n      </Tabs>\n\n      {/* Alertas */}\n      {!configuracao.emailAtivo && (\n        <Alert>\n          <AlertTriangle className=\"h-4 w-4\" />\n          <AlertDescription>\n            As notifica\xE7\xF5es por e-mail est\xE3o desativadas. Voc\xEA n\xE3o receber\xE1 nenhuma notifica\xE7\xE3o.\n          </AlertDescription>\n        </Alert>\n      )}\n      \n      {configuracao.tiposHabilitados.length === 0 && configuracao.emailAtivo && (\n        <Alert>\n          <Info className=\"h-4 w-4\" />\n          <AlertDescription>\n            Nenhum tipo de notifica\xE7\xE3o est\xE1 habilitado. Configure pelo menos um tipo para receber e-mails.\n          </A