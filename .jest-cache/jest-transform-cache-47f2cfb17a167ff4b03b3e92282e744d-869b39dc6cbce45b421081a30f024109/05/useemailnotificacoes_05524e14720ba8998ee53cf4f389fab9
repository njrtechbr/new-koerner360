    }
  }, [enviarNotificacaoAvaliacaoPendente, enviarLembretePrazo, enviarNotificacaoVencida, config]));
  /**
  * Atualiza estatsticas
  */
  const atualizarEstatisticas =
  /* istanbul ignore next */
  (cov_6rb6tgtmk().s[132]++, (0, _react.useCallback)(() => {
