      const tempoTotal =
      /* istanbul ignore next */
      (cov_6rb6tgtmk().s[124]++, Date.now() - inicioLote);
      const resultadoLote =
      /* istanbul ignore next */
      (cov_6rb6tgtmk().s[125]++, {
        total: notificacoes.length,
