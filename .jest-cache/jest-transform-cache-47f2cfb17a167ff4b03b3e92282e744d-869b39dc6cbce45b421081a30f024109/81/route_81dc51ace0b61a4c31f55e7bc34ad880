      /* istanbul ignore next */
      cov_29axsrvgw().b[23][0]++;
      cov_29axsrvgw().s[92]++;
      return _server.NextResponse.json({
        erro: 'Configurao de e-mail no encontrada'
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_29axsrvgw().b[23][1]++;
    }
    // Desativar configurao (no deletar)
    const configuracaoAtualizada =
    /* istanbul ignore next */
    (cov_29axsrvgw().s[93]++, await _prisma.prisma.configuracaoEmailNotificacao.update({
      where: {
        usuarioId
      },
      data: {
        ativo: false,
        emailsAtivos: false,
        atualizadoEm: new Date()
      }
    }));
    /* istanbul ignore next */
    cov_29axsrvgw().s[94]++;
    return _server.NextResponse.json({
      configuracao: configuracaoAtualizada,
      mensagem: 'Configurao de e-mail desativada com sucesso'
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_29axsrvgw().s[95]++;
    console.error('Erro ao desativar configurao de e-mail:', error);
    /* istanbul ignore next */
    cov_29axsrvgw().s[96]++;
    return _server.NextResponse.json({
      erro: 'Erro interno do servidor'
    }, {
      status: 500
    });
  }
