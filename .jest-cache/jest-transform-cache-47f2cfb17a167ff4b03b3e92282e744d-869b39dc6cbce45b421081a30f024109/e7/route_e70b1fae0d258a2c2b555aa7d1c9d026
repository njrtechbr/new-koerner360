}
async function PUT(request) {
  /* istanbul ignore next */
  cov_2d0r4f6yic().f[7]++;
  cov_2d0r4f6yic().s[38]++;
  try {
    const session =
    /* istanbul ignore next */
    (cov_2d0r4f6yic().s[39]++, await (0, _auth.auth)());
    /* istanbul ignore next */
    cov_2d0r4f6yic().s[40]++;
    if (!session?.user?.id) {
      /* istanbul ignore next */
      cov_2d0r4f6yic().b[5][0]++;
      cov_2d0r4f6yic().s[41]++;
      return _server.NextResponse.json({
        erro: 'Usuário não autenticado'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_2d0r4f6yic().b[5][1]++;
    }
    // Verificar permissões (apenas ADMIN pode alterar configurações)
    const temPermissao =
    /* istanbul ignore next */
    (cov_2d0r4f6yic().s[42]++, await (0, _permissoes.verificarPermissao)(session.user.id, ['ADMIN']));
    /* istanbul ignore next */
    cov_2d0r4f6yic().s[43]++;
    if (!temPermissao) {
      /* istanbul ignore next */
      cov_2d0r4f6yic().b[6][0]++;
      cov_2d0r4f6yic().s[44]++;
      return _server.NextResponse.json({
        erro: 'Sem permissão para alterar configurações do agendador'
      }, {
        status: 403
      });
    } else
    /* istanbul ignore next */
    {
      cov_2d0r4f6yic().b[6][1]++;
    }
    const body =
    /* istanbul ignore next */
    (cov_2d0r4f6yic().s[45]++, await request.json());
    const dadosValidados =
    /* istanbul ignore next */
    (cov_2d0r4f6yic().s[46]++, configuracaoAgendadorSchema.parse(body));
    const agendador =
    /* istanbul ignore next */
    (cov_2d0r4f6yic().s[47]++, obterAgendador());
    // Parar agendador se estiver ativo
    /* istanbul ignore next */
    cov_2d0r4f6yic().s[48]++;
    if (agendador.estaAtivo()) {
      /* istanbul ignore next */
      cov_2d0r4f6yic().b[7][0]++;
      cov_2d0r4f6yic().s[49]++;
      agendador.parar();
    } else
    /* istanbul ignore next */
    {
      cov_2d0r4f6yic().b[7][1]++;
    }
    // Atualizar configuração
    cov_2d0r4f6yic().s[50]++;
    agendador.atualizarConfiguracao(dadosValidados);
    // Reiniciar se deve estar ativo
    /* istanbul ignore next */
    cov_2d0r4f6yic().s[51]++;
    if (dadosValidados.ativo) {
      /* istanbul ignore next */
      cov_2d0r4f6yic().b[8][0]++;
      cov_2d0r4f6yic().s[52]++;
      agendador.iniciar();
    } else
    /* istanbul ignore next */
    {
      cov_2d0r4f6yic().b[8][1]++;
    }
    cov_2d0r4f6yic().s[53]++;
    return _server.NextResponse.json({
      configuracao: agendador.obterConfiguracao(),
      ativo: agendador.estaAtivo(),
      mensagem: 'Configuração do agendador atualizada com sucesso'
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_2d0r4f6yic().s[54]++;
    if (error instanceof _zod.z.ZodError) {
      /* istanbul ignore next */
      cov_2d0r4f6yic().b[9][0]++;
      cov_2d0r4f6yic().s[55]++;
      return _server.NextResponse.json({
        erro: 'Dados inválidos',
        detalhes: error.errors.map(err => {
          /* istanbul ignore next */
          cov_2d0r4f6yic().f[8]++;
          cov_2d0r4f6yic().s[56]++;
          return {
            campo: err.path.join('.'),
            mensagem: err.message
          };
        })
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_2d0r4f6yic().b[9][1]++;
    }
    cov_2d0r4f6yic().s[57]++;
    console.error('Erro ao atualizar configuração do agendador:', error);
    /* istanbul ignore next */
    cov_2d0r4f6yic().s[58]++;
    return _server.NextResponse.json({
      erro: 'Erro interno do servidor'
    }, {
      status: 500
    });
  }
}
