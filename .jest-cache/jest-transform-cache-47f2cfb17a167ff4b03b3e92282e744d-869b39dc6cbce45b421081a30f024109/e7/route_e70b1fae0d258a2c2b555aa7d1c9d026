    const limparTodas =
    /* istanbul ignore next */
    (cov_2d0r4f6yic().s[103]++, searchParams.get('limparTodas') === 'true');
    const agendador =
    /* istanbul ignore next */
    (cov_2d0r4f6yic().s[104]++, obterAgendador());
