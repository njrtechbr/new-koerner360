    /* istanbul ignore next */
    (cov_2d0r4f6yic().s[28]++, searchParams.get('estatisticas') === 'true');
    const incluirTarefas =
    /* istanbul ignore next */
    (cov_2d0r4f6yic().s[29]++, searchParams.get('tarefas') === 'true');
    const status =
    /* istanbul ignore next */
    (cov_2d0r4f6yic().s[30]++, {
      ativo: agendador.estaAtivo(),
      configuracao: agendador.obterConfiguracao()
    });
    /* istanbul ignore next */
    cov_2d0r4f6yic().s[31]++;
    if (incluirEstatisticas) {
      /* istanbul ignore next */
      cov_2d0r4f6yic().b[3][0]++;
      cov_2d0r4f6yic().s[32]++;
      status.estatisticas = agendador.obterEstatisticas();
    } else
    /* istanbul ignore next */
    {
      cov_2d0r4f6yic().b[3][1]++;
    }
    cov_2d0r4f6yic().s[33]++;
    if (incluirTarefas) {
      /* istanbul ignore next */
      cov_2d0r4f6yic().b[4][0]++;
      cov_2d0r4f6yic().s[34]++;
      status.tarefasPendentes = agendador.obterTarefasPendentes();
    } else
    /* istanbul ignore next */
