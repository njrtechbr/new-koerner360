        /* istanbul ignore next */
        cov_1qcwcqhmnc().s[62]++;
        break;
      case 'parar':
        /* istanbul ignore next */
        cov_1qcwcqhmnc().b[12][1]++;
        cov_1qcwcqhmnc().s[63]++;
        if (!agendador.estaAtivo()) {
          /* istanbul ignore next */
          cov_1qcwcqhmnc().b[14][0]++;
          cov_1qcwcqhmnc().s[64]++;
