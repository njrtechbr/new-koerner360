        mensagem: `Atualizao concluda: ${resultado.periodosAtualizados} perodo(s) atualizado(s)`,
        estatisticas: {
          periodosAtualizados: resultado.periodosAtualizados,
          periodosAtivados: resultado.periodosAtivados,
          periodosFinalizados: resultado.periodosFinalizados
        },
        detalhes: resultado.detalhes
      });
    }
  } catch (error) {
    /* istanbul ignore next */
    cov_2pcs236ny2().s[56]++;
    console.error('Erro na atualizao de status:', error);
    /* istanbul ignore next */
    cov_2pcs236ny2().s[57]++;
    if (error instanceof _zod.z.ZodError) {
      /* istanbul ignore next */
      cov_2pcs236ny2().b[24][0]++;
      cov_2pcs236ny2().s[58]++;
      return _server.NextResponse.json({
        erro: 'Parmetros invlidos',
        detalhes: error.errors
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_2pcs236ny2().b[24][1]++;
    }
    cov_2pcs236ny2().s[59]++;
    return _server.NextResponse.json({
      erro: 'Erro interno do servidor'
    }, {
      status: 500
    });
  }
}
async function GET(request) {
  /* istanbul ignore next */
  cov_2pcs236ny2().f[8]++;
  cov_2pcs236ny2().s[60]++;
