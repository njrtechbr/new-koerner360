    const periodos =
    /* istanbul ignore next */
    (cov_2pcs236ny2().s[74]++, await prisma.periodoAvaliacao.findMany({
      where: {
        status: {
          not: StatusPeriodo.CANCELADO
        }
      },
      select: {
        id: true,
        nome: true,
        status: true,
        dataInicio: true,
        dataFim: true
