                      \n                            <span className=\"text-xs text-muted-foreground\">\n                              {format(new Date(notificacao.criadoEm), 'dd/MM HH:mm', {\n                                locale: ptBR,\n                              })}\n                            </span>\n                          </div>\n                          \n                          {notificacao.prazo && (\n                            <span className=\"text-xs text-muted-foreground flex items-center gap-1\">\n                              <Clock className=\"h-3 w-3\" />\n                              {format(new Date(notificacao.prazo), 'dd/MM', {\n                                locale: ptBR,\n                              })}\n                            </span>\n                          )}\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                ))}\n              </div>\n            </ScrollArea>\n            \n            {/* Rodap\xE9 com a\xE7\xF5es */}\n            <div className=\"p-3 border-t bg-muted/20\">\n              <div className=\"flex items-center justify-between\">\n                <div className=\"text-xs text-muted-foreground\">\n                  {expandido ? (\n                    `${notificacoesFiltradas.length} de ${notificacoes.length} notifica\xE7\xF5es`\n                  ) : (\n                    `${Math.min(limite, notificacoesFiltradas.length)} de ${notificacoes.length} notifica\xE7\xF5es`\n                  )}\n                </div>\n                \n                <div className=\"flex items-center gap-2\">\n                  {notificacoes.length > limite && (\n                    <Button\n                      variant=\"ghost\"\n                      size=\"sm\"\n                      onClick={() => setExpandido(!expandido)}\n                      className=\"text-xs h-7\"\n                    >\n                      {expandido ? 'Mostrar