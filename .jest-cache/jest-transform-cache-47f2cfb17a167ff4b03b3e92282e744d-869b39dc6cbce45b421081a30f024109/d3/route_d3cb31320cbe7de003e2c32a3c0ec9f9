            erro: `Tipo de e-mail no suportado: ${tipo}`
          }, {
            status: 400
          });
      }
      /* istanbul ignore next */
      cov_10k1q9aos9().s[86]++;
      return _server.NextResponse.json({
        resultado,
        mensagem: resultado.sucesso ?
        /* istanbul ignore next */
        (cov_10k1q9aos9().b[44][0]++, `E-mail enviado com sucesso para ${resultado.sucessos} destinatrios`) :
        /* istanbul ignore next */
        (cov_10k1q9aos9().b[44][1]++, `Falha no envio: ${resultado.falhas} destinatrios com erro`)
      });
    }
  } catch (error) {
    /* istanbul ignore next */
    cov_10k1q9aos9().s[87]++;
    if (error instanceof _zod.z.ZodError) {
      /* istanbul ignore next */
      cov_10k1q9aos9().b[45][0]++;
      cov_10k1q9aos9().s[88]++;
      return _server.NextResponse.json({
        erro: 'Dados invlidos',
        detalhes: error.errors.map(err => {
          /* istanbul ignore next */
          cov_10k1q9aos9().f[6]++;
          cov_10k1q9aos9().s[89]++;
          return {
            campo: err.path.join('.'),
            mensagem: err.message
          };
        })
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_10k1q9aos9().b[45][1]++;
    }
    cov_10k1q9aos9().s[90]++;
    console.error('Erro ao enviar e-mail de notificao:', error);
    /* istanbul ignore next */
    cov_10k1q9aos9().s[91]++;
    return _server.NextResponse.json({
      erro: 'Erro interno do servidor'
    }, {
      status: 500
    });
  }
}
async function GET(request) {
  /* istanbul ignore next */
  cov_10k1q9aos9().f[7]++;
  cov_10k1q9aos9().s[92]++;
  try {
    const session =
    /* istanbul ignore next */
    (cov_10k1q9aos9().s[93]++, await (0, _auth.auth)());
    /* istanbul ignore next */
    cov_10k1q9aos9().s[94]++;
    if (!session?.user?.id) {
      /* istanbul ignore next */
      cov_10k1q9aos9().b[46][0]++;
      cov_10k1q9aos9().s[95]++;
      return _server.NextResponse.json({
        erro: 'Usurio no autenticado'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_10k1q9aos9().b[46][1]++;
    }
    // Verificar permisses
    const temPermissao =
    /* istanbul ignore next */
    (cov_10k1q9aos9().s[96]++, await (0, _permissoes.verificarPermissao)(session.user.id, ['ADMIN', 'GESTOR']));
    /* istanbul ignore next */
    cov_10k1q9aos9().s[97]++;
    if (!temPermissao) {
      /* istanbul ignore next */
      cov_10k1q9aos9().b[47][0]++;
      cov_10k1q9aos9().s[98]++;
      return _server.NextResponse.json({
        erro: 'Sem permisso para visualizar estatsticas de e-mail'
      }, {
        status: 403
      });
    } else
    /* istanbul ignore next */
    {
      cov_10k1q9aos9().b[47][1]++;
    }
    const gerenciadorEmail =
    /* istanbul ignore next */
    (cov_10k1q9aos9().s[99]++, (0, _emailnotificacoes.obterGerenciadorEmail)());
    const estatisticas =
    /* istanbul ignore next */
    (cov_10k1q9aos9().s[100]++, await gerenciadorEmail.obterEstatisticas());
    /* istanbul ignore next */
    cov_10k1q9aos9().s[101]++;
    return _server.NextResponse.json({
      estatisticas,
      mensagem: 'Estatsticas de e-mail obtidas com sucesso'
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_10k1q9aos9().s[102]++;
    console.error('Erro ao obter estatsticas de e-mail:', error);
    /* istanbul ignore next */
    cov_10k1q9aos9().s[103]++;
    return _server.NextResponse.json({
      erro: 'Erro interno do servidor'
    }, {
      status: 500
    });
  }
}
