      /* istanbul ignore next */
      cov_2en0qbbi9j().s[54]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_2en0qbbi9j().b[4][1]++;
    }
    // Agendar lembretes de antecedncia
    cov_2en0qbbi9j().s[55]++;
    for (const dias of this.configuracao.diasAntecedencia) {
      const dataLembrete =
      /* istanbul ignore next */
      (cov_2en0qbbi9j().s[56]++, (0, _datefns.addDays)(prazoAvaliacao, -dias));
      /* istanbul ignore next */
