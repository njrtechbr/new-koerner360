  /**
  * Busca avaliaes que precisam de lembretes
  */
  async buscarAvaliacoesPendentes() {
    /* istanbul ignore next */
    cov_2en0qbbi9j().f[11]++;
    const agora =
    /* istanbul ignore next */
    (cov_2en0qbbi9j().s[47]++, new Date());
    const em30Dias =
    /* istanbul ignore next */
    (cov_2en0qbbi9j().s[48]++, (0, _datefns.addDays)(agora, 30)); // Buscar avaliaes com prazo nos prximos 30 dias
    /* istanbul ignore next */
    cov_2en0qbbi9j().s[49]++;
    return await _prisma.prisma.avaliacao.findMany({
      where: {
