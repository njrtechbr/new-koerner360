    /* istanbul ignore next */
    (cov_2en0qbbi9j().b[8][0]++, !this.configuracao.incluirFimDeSemana) &&
    /* istanbul ignore next */
    (cov_2en0qbbi9j().b[8][1]++, this.isFimDeSemana(dataEnvioComHorario))) {
