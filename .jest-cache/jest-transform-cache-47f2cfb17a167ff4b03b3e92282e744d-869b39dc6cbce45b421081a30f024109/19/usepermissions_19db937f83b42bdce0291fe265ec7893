    podeImportarUsuarios,
    podeExportarUsuarios,
    // Utilitários
    obterPermissoes,
    obterTipoUsuario,
    // Estado da sessão
    carregando: status === 'loading',
    autenticado: status === 'authenticated',
    usuario: session?.user
  };
}
