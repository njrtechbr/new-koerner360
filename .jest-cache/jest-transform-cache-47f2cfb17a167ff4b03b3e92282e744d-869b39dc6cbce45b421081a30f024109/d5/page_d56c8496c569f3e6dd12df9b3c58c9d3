      try {
        const response =
        /* istanbul ignore next */
        (cov_ag4b328qf().s[29]++, await fetch('/api/auth/verify-reset-token', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({
            token
          })
        }));
        /* istanbul ignore next */
        cov_ag4b328qf().s[30]++;
        setTokenValido(response.ok);
      } catch (error) {
        /* istanbul ignore next */
        cov_ag4b328qf().s[31]++;
        console.error('Erro ao verificar token:', error);
        /* istanbul ignore next */
        cov_ag4b328qf().s[32]++;
        setTokenValido(false);
      }
    };
    /* istanbul ignore next */
    cov_ag4b328qf().s[33]++;
    verificarToken();
  }, [token]);
  /* istanbul ignore next */
  cov_ag4b328qf().s[34]++;
  const handleSubmit = async e => {
    /* istanbul ignore next */
    cov_ag4b328qf().f[5]++;
    cov_ag4b328qf().s[35]++;
    e.preventDefault();
    /* istanbul ignore next */
    cov_ag4b328qf().s[36]++;
    if (
    /* istanbul ignore next */
    (cov_ag4b328qf().b[4][0]++, !novaSenha) ||
    /* istanbul ignore next */
    (cov_ag4b328qf().b[4][1]++, !confirmarSenha)) {
      /* istanbul ignore next */
      cov_ag4b328qf().b[3][0]++;
      cov_ag4b328qf().s[37]++;
      _sonner.toast.error('Por favor, preencha todos os campos');
      /* istanbul ignore next */
      cov_ag4b328qf().s[38]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ag4b328qf().b[3][1]++;
    }
    cov_ag4b328qf().s[39]++;
    if (novaSenha.length < 6) {
      /* istanbul ignore next */
      cov_ag4b328qf().b[5][0]++;
      cov_ag4b328qf().s[40]++;
      _sonner.toast.error('A senha deve ter pelo menos 6 caracteres');
      /* istanbul ignore next */
      cov_ag4b328qf().s[41]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ag4b328qf().b[5][1]++;
    }
    cov_ag4b328qf().s[42]++;
    if (novaSenha !== confirmarSenha) {
      /* istanbul ignore next */
      cov_ag4b328qf().b[6][0]++;
      cov_ag4b328qf().s[43]++;
      _sonner.toast.error('As senhas não coincidem');
      /* istanbul ignore next */
      cov_ag4b328qf().s[44]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ag4b328qf().b[6][1]++;
    }
    cov_ag4b328qf().s[45]++;
    setCarregando(true);
    /* istanbul ignore next */
    cov_ag4b328qf().s[46]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_ag4b328qf().s[47]++, await fetch('/api/auth/reset-password', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify({
          token,
          novaSenha
        })
      }));
      const data =
      /* istanbul ignore next */
      (cov_ag4b328qf().s[48]++, await response.json());
      /* istanbul ignore next */
      cov_ag4b328qf().s[49]++;
      if (response.ok) {
        /* istanbul ignore next */
        cov_ag4b328qf().b[7][0]++;
        cov_ag4b328qf().s[50]++;
        _sonner.toast.success('Senha redefinida com sucesso!');
        /* istanbul ignore next */
        cov_ag4b328qf().s[51]++;
        router.push('/auth/login');
      } else {
        /* istanbul ignore next */
        cov_ag4b328qf().b[7][1]++;
        cov_ag4b328qf().s[52]++;
        _sonner.toast.error(
        /* istanbul ignore next */
        (cov_ag4b328qf().b[8][0]++, data.message) ||
        /* istanbul ignore next */
        (cov_ag4b328qf().b[8][1]++, 'Erro ao redefinir senha'));
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_ag4b328qf().s[53]++;
      console.error('Erro na redefinição de senha:', error);
      /* istanbul ignore next */
      cov_ag4b328qf().s[54]++;
      _sonner.toast.error('Erro interno do servidor');
    } finally {
      /* istanbul ignore next */
      cov_ag4b328qf().s[55]++;
      setCarregando(false);
    }
  };
  /* istanbul ignore next */
  cov_ag4b328qf().s[56]++;
  if (tokenValido === null) {
    /* istanbul ignore next */
    cov_ag4b328qf().b[9][0]++;
    cov_ag4b328qf().s[57]++;
    return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "min-h-screen flex items-center justify-center bg-background p-4",
      children: /*#__PURE__*/(0, _jsxruntime.jsx)(_card.Card, {
        className: "w-full max-w-md",
        children: /*#__PURE__*/(0, _jsxruntime.jsx)(_card.CardContent, {
          className: "pt-6",
          children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "flex items-center justify-center",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Loader2, {
              className: "h-6 w-6 animate-spin"
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
              className: "ml-2",
              children: "Verificando token..."
            })]
          })
        })
      })
    });
  } else
  /* istanbul ignore next */
  {
    cov_ag4b328qf().b[9][1]++;
  }
  cov_ag4b328qf().s[58]++;
  if (tokenValido === false) {
    /* istanbul ignore next */
    cov_ag4b328qf().b[10][0]++;
    cov_ag4b328qf().s[59]++;
    return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "min-h-screen flex items-center justify-center bg-background p-4",
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)(_card.Card, {
        className: "w-full max-w-md",
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)(_card.CardHeader, {
          className: "text-center",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_card.CardTitle, {
            className: "text-2xl font-bold text-destructive",
            children: "Token Inv\xe1lido"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)(_card.CardDescription, {
            children: "O link de recupera\xe7\xe3o \xe9 inv\xe1lido ou expirou. Solicite um novo link de recupera\xe7\xe3o."
          })]
        }), /*#__PURE__*/(0, _jsxruntime.jsx)(_card.CardContent, {
          children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-4",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_link.default, {
              href: "/auth/forgot-password",
              children: /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
                className: "w-full",
                children: "Solicitar Nova Recupera\xe7\xe3o"
              })
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
              className: "text-center",
              children: /*#__PURE__*/(0, _jsxruntime.jsxs)(_link.default, {
                href: "/auth/login",
                className: "text-sm text-primary hover:underline inline-flex items-center gap-1",
                children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.ArrowLeft, {
                  className: "h-4 w-4"
                }), "Voltar ao login"]
              })
            })]
          })
        })]
      })
    });
  } else
  /* istanbul ignore next */
  {
    cov_ag4b328qf().b[10][1]++;
  }
  cov_ag4b328qf().s[60]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
    className: "min-h-screen flex items-center justify-center bg-background p-4",
    children: /*#__PURE__*/(0, _jsxruntime.jsxs)(_card.Card, {
      className: "w-full max-w-md",
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)(_card.CardHeader, {
        className: "text-center",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_card.CardTitle, {
          className: "text-2xl font-bold",
          children: "Redefinir Senha"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)(_card.CardDescription, {
          children: "Digite sua nova senha abaixo."
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)(_card.CardContent, {
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("form", {
          onSubmit: handleSubmit,
          className: "space-y-4",
          children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-2",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_label.Label, {
              htmlFor: "novaSenha",
              children: "Nova Senha"
            }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              className: "relative",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_input.Input, {
                id: "novaSenha",
                type: mostrarSenha ?
                /* istanbul ignore next */
                (cov_ag4b328qf().b[11][0]++, 'text') :
                /* istanbul ignore next */
                (cov_ag4b328qf().b[11][1]++, 'password'),
                placeholder: "Digite sua nova senha",
                value: novaSenha,
                onChange: e => {
                  /* istanbul ignore next */
                  cov_ag4b328qf().f[6]++;
                  cov_ag4b328qf().s[61]++;
                  return setNovaSenha(e.target.value);
                },
                required: true,
                disabled: carregando,
                minLength: 6
              }), /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
                type: "button",
                variant: "ghost",
                size: "sm",
                className: "absolute right-0 top-0 h-full px-3 py-2 hover:bg-transparent",
                onClick: () => {
                  /* istanbul ignore next */
                  cov_ag4b328qf().f[7]++;
                  cov_ag4b328qf().s[62]++;
                  return setMostrarSenha(!mostrarSenha);
                },
                disabled: carregando,
                children: mostrarSenha ?
                /* istanbul ignore next */
                (/*#__PURE__*/cov_ag4b328qf().b[12][0]++, (0, _jsxruntime.jsx)(_lucidereact.EyeOff, {
                  className: "h-4 w-4"
                })) :
                /* istanbul ignore next */
                (/*#__PURE__*/cov_ag4b328qf().b[12][1]++, (0, _jsxruntime.jsx)(_lucidereact.Eye, {
                  className: "h-4 w-4"
                }))
              })]
            })]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-2",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_label.Label, {
              htmlFor: "confirmarSenha",
              children: "Confirmar Nova Senha"
            }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              className: "relative",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_input.Input, {
                id: "confirmarSenha",
                type: mostrarConfirmacao ?
                /* istanbul ignore next */
                (cov_ag4b328qf().b[13][0]++, 'text') :
                /* istanbul ignore next */
                (cov_ag4b328qf().b[13][1]++, 'password'),
                placeholder: "Confirme sua nova senha",
                value: confirmarSenha,
                onChange: e => {
                  /* istanbul ignore next */
                  cov_ag4b328qf().f[8]++;
                  cov_ag4b328qf().s[63]++;
                  return setConfirmarSenha(e.target.value);
                },
                required: true,
                disabled: carregando,
                minLength: 6
              }), /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
                type: "button",
                variant: "ghost",
                size: "sm",
                className: "absolute right-0 top-0 h-full px-3 py-2 hover:bg-transparent",
                onClick: () => {
                  /* istanbul ignore next */
                  cov_ag4b328qf().f[9]++;
                  cov_ag4b328qf().s[64]++;
                  return setMostrarConfirmacao(!mostrarConfirmacao);
                },
                disabled: carregando,
                children: mostrarConfirmacao ?
                /* istanbul ignore next */
                (/*#__PURE__*/cov_ag4b328qf().b[14][0]++, (0, _jsxruntime.jsx)(_lucidereact.EyeOff, {
                  className: "h-4 w-4"
                })) :
                /* istanbul ignore next */
                (/*#__PURE__*/cov_ag4b328qf().b[14][1]++, (0, _jsxruntime.jsx)(_lucidereact.Eye, {
                  className: "h-4 w-4"
                }))
              })]
            })]
          }), /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
            type: "submit",
            className: "w-full",
            disabled: carregando,
            children: carregando ?
            /* istanbul ignore next */
            (/*#__PURE__*/cov_ag4b328qf().b[15][0]++, (0, _jsxruntime.jsxs)(_jsxruntime.Fragment, {
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Loader2, {
                className: "mr-2 h-4 w-4 animate-spin"
              }), "Redefinindo..."]
            })) :
            /* istanbul ignore next */
            (cov_ag4b328qf().b[15][1]++, 'Redefinir Senha')
          })]
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "mt-4 text-center",
          children: /*#__PURE__*/(0, _jsxruntime.jsxs)(_link.default, {
            href: "/auth/login",
            className: "text-sm text-primary hover:underline inline-flex items-center gap-1",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.ArrowLeft, {
              className: "h-4 w-4"
            }), "Voltar ao login"]
          })
        })]
      })]
    })
  });
}
