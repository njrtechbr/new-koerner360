            status: 400
          });
        } else
        /* istanbul ignore next */
        {
          cov_1682j1o6ow().b[26][1]++;
        }
        cov_1682j1o6ow().s[102]++;
        await _prisma.prisma.lembreteAgendado.update({
          where: {
            id
          },
          data: {
            dataEnvio: new Date(novaData),
            enviado: false,
            erro: null
          }
        });
        /* istanbul ignore next */
        cov_1682j1o6ow().s[103]++;
        resultado = {
          mensagem: 'Lembrete reagendado com sucesso',
