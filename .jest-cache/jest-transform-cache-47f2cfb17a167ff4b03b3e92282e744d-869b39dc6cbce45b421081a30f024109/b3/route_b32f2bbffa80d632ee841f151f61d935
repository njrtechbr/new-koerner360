      /* istanbul ignore next */
      cov_1682j1o6ow().b[21][0]++;
      cov_1682j1o6ow().s[80]++;
      return _server.NextResponse.json({
        erro: 'No autorizado'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_1682j1o6ow().b[21][1]++;
    }
    // Verificar permisses (apenas ADMIN e GESTOR)
    cov_1682j1o6ow().s[81]++;
    if (!['ADMIN', 'GESTOR'].includes(session.user.papel)) {
      /* istanbul ignore next */
      cov_1682j1o6ow().b[22][0]++;
      cov_1682j1o6ow().s[82]++;
