        break;
      case 'marcar_enviado':
        /* istanbul ignore next */
        cov_1682j1o6ow().b[24][1]++;
        cov_1682j1o6ow().s[94]++;
        if (lembrete.enviado) {
          /* istanbul ignore next */
          cov_1682j1o6ow().b[25][0]++;
          cov_1682j1o6ow().s[95]++;
          return _server.NextResponse.json({
            erro: 'Lembrete j foi marcado como enviado'
          }, {
            status: 400
          });
        } else
        /* istanbul ignore next */
        {
          cov_1682j1o6ow().b[25][1]++;
