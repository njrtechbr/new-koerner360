          erro: 'Ao no reconhecida'
        }, {
          status: 400
        });
    }
    /* istanbul ignore next */
    cov_1682j1o6ow().s[106]++;
    return _server.NextResponse.json({
      sucesso: true,
      ...resultado,
      dados: {
        id: lembrete.id,
