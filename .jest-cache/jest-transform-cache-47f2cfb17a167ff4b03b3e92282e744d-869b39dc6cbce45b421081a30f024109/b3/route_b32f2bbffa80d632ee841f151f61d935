      cov_1682j1o6ow().s[88]++;
      return _server.NextResponse.json({
        erro: 'Lembrete no encontrado'
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_1682j1o6ow().b[23][1]++;
    }
    let resultado =
    /* istanbul ignore next */
    (cov_1682j1o6ow().s[89]++, {});
    /* istanbul ignore next */
    cov_1682j1o6ow().s[90]++;
    switch (acao) {
      case 'reenviar':
        /* istanbul ignore next */
        cov_1682j1o6ow().b[24][0]++;
        cov_1682j1o6ow().s[91]++;
        // Implementar reenvio do lembrete
        await _prisma.prisma.lembreteAgendado.update({
          where: {
            id
          },
